GROUP BY : 
KOMUT SATIRIMIZDA AYNI DEĞERLERE SAHİP BİLGİLERİ BİZE DÖNDÜRÜR.

SELECT KOLON1,KOLON2,SUM(PRICE) FROM TABLO ADI GROUP BY KOLON1,KOLON2 ŞEKLİNDE KULLANILIR.

ÖRNEK : 
SELECT MAX(AGE) AS MAKSİMUM_YAS ,MIN(AGE) AS MİNİMUM_YAS FROM CUSTOMER GROUP BY CITY

ÖRNEK :
SELECT BIRTDATE ,COUNT(GENDER) AS CİNSİYET FROM USERS GROUP BY BIRTDATE
