ORNEK :

WITH CTEADRESS (USER_NUMBER,ADRESLERİ) AS
(
SELECT USER_ID,DISTRICT FROM ADRESS
)
SELECT *FROM CTEADRESS
-------------------------------------------------------------------
ORNEK : 

WITH CTEADRESS (USER_NUMBER,ADRESLERİ) AS
(
SELECT USER_ID,(DISTRICT ) FROM ADRESS WHERE DISTRICT BETWEEN 400 AND 600
)
SELECT *FROM CTEADRESS

